PORT=4000
MEDIASOUP_WS_PATH=/ws
API_BASE_URL=http://localhost:3001
VOICE_TOKEN_SECRET=dev-voice-secret-change-me
VOICE_INTERNAL_SECRET=dev-voice-internal-secret-change-me
DEBUG_VOICE=false

# Network / NAT
# Local dev: 127.0.0.1
# Public/NAT: 0.0.0.0 (requires MEDIASOUP_ANNOUNCED_ADDRESS)
MEDIASOUP_LISTEN_IP=127.0.0.1
MEDIASOUP_ANNOUNCED_ADDRESS=
MEDIASOUP_MIN_PORT=40000
MEDIASOUP_MAX_PORT=49999
MEDIASOUP_INITIAL_AVAILABLE_OUTGOING_BITRATE=1000000

# Optional override (default is CPU core count)
MEDIASOUP_WORKER_COUNT=

# Optional: "all" or "relay"
ICE_TRANSPORT_POLICY=
